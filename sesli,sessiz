#include <stdio.h>
#include <stdlib.h>



int main() {
    char harf;
    printf("Bir harf gir: ");
    scanf("%c", &harf);

    if (harf == 'a' || harf == 'e' || harf == 'i' || harf == 'o' || harf == 'u' || harf == 'ı' || harf == 'ö' || harf == 'ü') {
        printf("Sesli harf\n");
    } else {
        printf("Sessiz harf\n");
    }

    return 0;
}

